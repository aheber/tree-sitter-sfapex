    "limit_usage": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "limit"
      }
    },
    "limit": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^:]+"
            },
            "named": true,
            "value": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "consumed",
          "content": {
            "type": "SYMBOL",
            "name": "number"
          }
        },
        {
          "type": "STRING",
          "value": "out of"
        },
        {
          "type": "FIELD",
          "name": "available",
          "content": {
            "type": "SYMBOL",
            "name": "number"
          }
        }
      ]
    },
    "event_details": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "event_detail"
      }
    },
