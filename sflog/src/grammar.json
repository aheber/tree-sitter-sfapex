    "event_detail_value": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PATTERN",
          "value": "(\\n(  |[^\\d]))?[^\\n|]+"
        }
      }
    },
    "timestamp": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "time"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "time": {
      "type": "PATTERN",
      "value": "\\d{2}:\\d{2}:\\d{2}\\.\\d{1,3}"
    },
    "duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "event_identifier": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "\\w+"
